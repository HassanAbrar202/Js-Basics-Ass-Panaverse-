q42
const org = ["hassan","ahmed","khan"];
const show_magicians = (arr)=>
{
    arr.map((i)=>
    {
        console.log("the magician is: ",i)
    })
}
const make_great=(arr)=>
{
    arr.map((i,j)=>
    {
        arr[j]=arr[j].concat(" the great");
    })
}
show_magicians(org);
make_great(org);
show_magicians(org);

q43
const org = ["hassan","ahmed","khan"];
const copyorg=[...org];
const show_magicians = (arr)=>
{
    arr.map((i)=>
    {
        console.log("the magician is: ",i)
    })
}
const make_great=(arr)=>
{
    arr.map((i,j)=>
    {
        arr[j]=arr[j].concat(" the great");
    })
}
show_magicians(org);
make_great(copyorg);
show_magicians(copyorg);

q44
const make_sandwich = (...items)=>
{
    console.log("the ordered sandwich is: ")
    items.map((i)=>
    {
        console.log("with "+i)
    })
}
make_sandwich('cheese', 'lettuce','chicken');
make_sandwich('cheese', 'lettuce','beef','jalapeno');
make_sandwich('cheese', 'lettuce','chicken',"spice","mayonaise","olives");

q45
const make_car=(constructor, model,options)=>
{
    const car = {
        constructor: constructor,
        model: model,
        }
        Object.entries(options).map((i)=>
        {
            car[`${i[0]}`]=i[1];
        })
    return car;
}
var my_outback = make_car('toyota', 'corolla', {color:'blue',price:"10000"})
console.log(my_outback)
my_outback = make_car('suzuki', 'mehran', {color:'black',price:"10000",comfortable:true})
console.log(my_outback)
my_outback = make_car('hyundai', 'sonata', {color:'grey',price:"10000",engine:true})
console.log(my_outback)