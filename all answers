q15
let guests = new Array();
guests.push("Aslam","Akram","Saima");
console.log(guests);
guests.map((i)=>{
    console.log(i+" I would like to invite you on Dinner.");
})
console.log('Saima could not make it');
let newGuests = new Array(); 
newGuests = guests.filter(i=>i!=="Saima");
console.log(newGuests);
newGuests.push("Fatima");
newGuests.map((i)=>{
    console.log(i+" I would like to invite you on Dinner.");
})

q16
let guests = new Array();
guests.push("Aslam","Akram","Saima");
// console.log(guests);
guests.map((i)=>{
    console.log(i+" I would like to invite you on Dinner.");
})
console.log('Saima could not make it');
let newGuests = new Array(); 
newGuests = guests.filter(i=>i!=="Saima");
console.log(newGuests);
newGuests.push("Fatima");
newGuests.map((i)=>{
    console.log(i+" I would like to invite you on Dinner.");
})
newGuests.unshift("Hassan");
newGuests.splice(Math.abs(newGuests.length/2),0,"Rayyan");
newGuests.push("Ahmed");
newGuests.map((i)=>{
    console.log(i+" I have found a bigger dinner table so I would like to invite you on Dinner.");
})
console.log(newGuests);

q17
let guests = new Array();
guests.push("Aslam","Akram","Saima");
// console.log(guests);
// guests.map((i)=>{
//     console.log(i+" I would like to invite you on Dinner.");
// })
console.log('Saima could not make it');
let newGuests = new Array(); 
newGuests = guests.filter(i=>i!=="Saima");
console.log(newGuests);
newGuests.push("Fatima");
newGuests.map((i)=>{
    console.log(i+" I would like to invite you on Dinner.");
})
newGuests.unshift("Hassan");
newGuests.splice(Math.abs(newGuests.length/2),0,"Rayyan");
newGuests.push("Ahmed");
newGuests.map((i)=>{
    console.log(i+" I have found a bigger dinner table so I would like to invite you on Dinner.");
})
console.log(newGuests);

console.log("you can invite only two people for dinner.");
for(var i=0;i<=newGuests.length;i++)
{
    if(newGuests.length > 2)
    {
        console.log("we are sorry we cannot invite you "+newGuests.pop());
    }
}
console.log(newGuests);
for(var i=0;i<=newGuests.length;i++)
{
        console.log("to empty array "+newGuests.pop());
}

q18
let places = ["Pakistan","UAE","Australia","Canada","America"];
console.log("original",places);
console.log("alphabetical sort",[...places].sort());
console.log("original",places);
console.log("alphabetical reverse sort",[...places].sort().reverse());
console.log("original",places);
console.log("sort",places.reverse());
console.log("original",places);
console.log("sort",places.reverse());
console.log("original",places);
console.log("alphabetical sort",places.sort());
console.log("original",places);
console.log("alphabetical reverse sort",places.sort().reverse());
console.log("original",places);

