if(alien_color[Math.floor(Math.random()*alien_color.length)] == "red")
{
    console.log("you have earned 5 points");
}
else
{
     console.log("you have earned 10 points");
}

q27
let alien_color = ["green","yellow","red"]
random_alien_color = alien_color[Math.floor(Math.random()*alien_color.length)];
console.log(random_alien_color);
if(random_alien_color == "green")
{
    console.log("you have earned 5 points");
}
else if (random_alien_color == "yellow")
{
     console.log("you have earned 10 points");
}
else if (random_alien_color == "red")
{
    console.log("you have earned 15 points");
}

q28
random_age = Math.floor(Math.random()*100)
console.log(random_age);
if( random_age < 2)
{
    console.log("person is a baby");
}
else if(random_age >= 2 && random_age<4)
{
    console.log("the person is a toddler.")
}
else if(random_age >= 4 && random_age<13)
{
    console.log("the person is a kid.")
}
else if(random_age >= 13 && random_age<20)
{
    console.log("the person is a teenager.")
}
else if(random_age >= 20 && random_age<65)
{
    console.log("the person is a adult.")
}
else if(random_age<=65)
{
    console.log("the person is a elder.")
}

