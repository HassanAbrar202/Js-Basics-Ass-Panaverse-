q3
let personName = "I m Hassan"; function titleCase(str) { str = str.toLowerCase() .split(' ') .map(function(word) { return (word.charAt(0).toUpperCase() + word.slice(1)); }); return str.join(' '); } console.log(personName.toLowerCase()); console.log(personName.toUpperCase()); console.log(titleCase(personName)); 

