q30
names=["admin","eric","hassan","aslam","admin"];
names.map((i)=>
{
    if(i == "admin")
    {
        console.log("Hello admin, would you like to see a status report?");
    }
    else
    {
        console.log(`Hello ${i}, thank you for logging in again.`)
    }
})

