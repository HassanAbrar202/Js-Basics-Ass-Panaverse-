q40
const make_album = (artist,title)=>
{
     album_dict = {
        artist: artist,
        title: title,
        }
    return album_dict
}
album = make_album('hassan', 'twinkle')
console.log(album);
album = make_album('ahmed', 'super')
console.log(album);
album = make_album('khan', 'yoman')
console.log(album);

